{
    "extends": "airbnb-base",
    "env": {
        "node": true,
        "es6": true
    },
    "rules": {
        "object-curly-newline": [
            "error",
            {
                "ObjectExpression": "always",
                "ObjectPattern": {
                    "multiline": true
                }
            }
        ],
        "new-cap": 0
    }
}